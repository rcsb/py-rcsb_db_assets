save__rcsb_polymer_instance_feature.type
    _item_description.description
;              A type or category of the feature.
;
    _item.name               '_rcsb_polymer_instance_feature.type'
    _item.category_id         rcsb_polymer_instance_feature
    _item.mandatory_code      yes
    _item_type.code           line
    loop_
    _item_examples.case        'CATH' 'SCOP'
    loop_
    _item_enumeration.value
    _item_enumeration.detail
    'CATH'    'CATH - Class, Architecture, Topology, and Homology Protein Structure Database'
    'SCOP'    'SCOP - Structural Classification of Proteins'
    'HELIX_P' 'Protein helices'
    'SHEET'   'Beta-sheets'
    'UNASSIGNED_SEC_STRUCT' 'Unassigned secondary structure'
    'CIS-PEPTIDE' 'Peptide linkages with CIS configurations'
    'BINDING_SITE' 'Macromolecular target for ligand binding'
    'ZERO_OCCUPANCY_RESIDUE_XYZ' "Residue coordinates with zero occupancy"
    'ZERO_OCCUPANCY_ATOM_XYZ'    "Atom coordinates with zero occupancy"
    'UNOBSERVED_ATOM_XYZ'        "Unobserved atom coordinates"
    'UNOBSERVED_RESIDUE_XYZ'     "Unobserved residue coordinates"
    #
    "ROTAMER_OUTLIER"       "Molprobity rotamer outlier"
    "RAMACHANDRAN_OUTLIER"  "Molprobity Ramachandran outlier"
    "RSRZ_OUTLIER"          "Real space R-value Z score > 2"
    "RSCC_OUTLIER"         "Real space density correlation value < 0.65"
    "MOGUL_BOND_OUTLIER"    "Mogul bond distance outlier"
    "MOGUL_ANGLE_OUTLIER"   "Mogul bond angle outlier"
    "BOND_OUTLIER"          "Molprobity bond distance outlier"
    "ANGLE_OUTLIER"         "Molprobity bond angle outlier"
    "STEREO_OUTLIER"        "Stereochemical/chirality outlier"
     save_

